  "dependencies": {
    "leaflet": "^0.7.3",
    "reqwest": "^1.1.4"
  },
  "devDependencies": {
    "browserify-shim": "^3.7.0",
    "grunt": "^0.4.5",
    "grunt-browserify": "^3.0.1",
    "grunt-contrib-uglify": "^0.6.0"
